package com.example.newhappyappy;

import com.orm.dsl.Table;

@Table
public class Book {
  private Long id;
private String title;
private String edition;

  public Database(){
  }

  public Database(String title, String edition){
     this.title = title;
     this.edition = edition;
  }

  public Long getId() {
      return id;
  }
  
  Database book = new Database("Title here", "2nd edition")
		  book.save();
}